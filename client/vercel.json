{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "builds": [
                {
                    "src": ".js",
                    "use": "@vercel/node"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}